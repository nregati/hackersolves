<code_scheme name="Default (1)">
  <option name="USE_FQ_CLASS_NAMES" value="true"/>
  <option name="CLASS_COUNT_TO_USE_IMPORT_ON_DEMAND" value="500"/>
  <JavaCodeStyleSettings>
    <option name="CLASS_NAMES_IN_JAVADOC" value="2"/>
  </JavaCodeStyleSettings>
  <codeStyleSettings language="JAVA">
    <arrangement>
      <groups>
        <group>
          <type>GETTERS_AND_SETTERS</type>
          <order>KEEP</order>
        </group>
        <group>
          <type>OVERRIDDEN_METHODS</type>
          <order>KEEP</order>
        </group>
        <group>
          <type>DEPENDENT_METHODS</type>
          <order>BREADTH_FIRST</order>
        </group>
      </groups>
    </arrangement>
  </codeStyleSettings>
</code_scheme>